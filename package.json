    "next-auth": "^4.24.13",
    "openai": "^4.104.0",
    "prisma": "^5.22.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.71.1",
    "recharts": "^3.7.0",
    "resend": "^6.8.0",
    "sonner": "^2.0.7",
    "stripe": "^20.2.0",
    "tailwind-merge": "^3.4.0",
    "xlsx": "^0.18.5",
